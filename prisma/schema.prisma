generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int      @id @default(autoincrement())
  firstname     String
  lastname      String
  employed      String
  date          String
  active        Boolean  @default(true)
  createdAt     DateTime @default(now())
  updatedAt     DateTime @default(now())
} 